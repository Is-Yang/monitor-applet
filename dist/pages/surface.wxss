page{background-color:#FAF7FA}.common-wrap{padding:30rpx 20rpx}.common-wrap .area-site .row{margin-bottom:5rpx;font-size:30rpx;color:#333}.common-wrap .area-site .row text{width:180rpx;display:inline-block;text-align:right;color:#666}.common-wrap .panel-wrap{margin-top:30rpx;background-color:#ffffff;box-shadow:0rpx 0rpx 11rpx 1rpx rgba(204,204,204,0.2);border-radius:6rpx}.common-wrap .panel-wrap .panel-title{display:flex;justify-content:space-between;padding:18rpx 20rpx;font-size:28rpx;color:#333}.common-wrap .panel-wrap .panel-title .more{width:100rpx;color:#999;position:relative}.common-wrap .panel-wrap .panel-title .more:after{content:'\F00A';position:absolute;top:0;right:0;font-family:'vant-icon';color:#ccc;font-size:36rpx}.common-wrap .panel-wrap .panel-body{display:flex;flex-wrap:wrap;margin:0 18rpx;padding:15rpx 0;border-top:1px solid #e5e5e5}.common-wrap .panel-wrap .panel-body .item{margin:10rpx 0;width:33.333%;font-size:26rpx;color:#333;text-align:center}