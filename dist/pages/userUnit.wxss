page {
  background-color: #FAF7FA;
}
.user-unit-container .head-wrap {
  position: fixed;
  width: 100%;
  height: 320rpx;
  z-index: 1;
  background-color: #FAF7FA;
}
.user-unit-container .head-wrap .user-wrap {
  position: absolute;
  right: 27rpx;
  bottom: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 2;
}
.user-unit-container .head-wrap .user-wrap image {
  width: 117rpx;
  height: 117rpx;
  background-color: #ffffff;
}
.user-unit-container .head-wrap .user-wrap .mark {
  margin-right: 21rpx;
  font-size: 21rpx;
  color: #fff;
  text-align: right;
}
.user-unit-container .head-wrap .user-wrap .mark .flag {
  display: inline-block;
  padding: 0 20rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 12rpx;
  background-image: linear-gradient(0deg, #ff8800 0%, #ffcf21 100%), linear-gradient(#52cc7a, #52cc7a);
  border-radius: 25px;
  font-size: 16rpx;
  text-align: center;
}
.user-unit-container .head-wrap .bg {
  width: 100%;
  height: 280rpx;
}
.user-unit-container .panel-wrap {
  margin-bottom: 20rpx;
  font-size: 26rpx;
  border-bottom: 1px solid #f7f7f7;
}
.user-unit-container .panel-wrap .title {
  font-size: 21rpx;
  color: #666;
  padding: 15rpx 23rpx;
}
.user-unit-container .panel-wrap .icon {
  width: 53rpx;
  height: 53rpx;
  vertical-align: middle;
}
.user-unit-container .panel-wrap .nickname {
  display: inline-block;
  min-width: 100rpx;
  margin-left: 10rpx;
  color: #333;
}
.user-unit-container .panel-wrap .tel {
  color: #999;
}
.user-unit-container .panel-wrap .num {
  padding-right: 20rpx;
}
.user-unit-container .panel-wrap .van-collapse-item__content {
  padding: 0 0 0 75rpx;
}
.user-unit-container .operate-cell .cell {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 21rpx 26rpx;
  height: 88rpx;
  border-bottom: 1px solid #f7f7f7;
  font-size: 26rpx;
  color: #333;
  box-sizing: border-box;
  background-color: #fff;
}
.user-unit-container .operate-cell .cell:last-child {
  border-bottom: none;
}
.user-unit-container .operate-cell .cell image {
  width: 48rpx;
  height: 48rpx;
  vertical-align: middle;
  margin-right: 15rpx;
}
.user-unit-container .operate-cell .cell input,
.user-unit-container .operate-cell .cell .right-txt {
  text-align: right;
  font-size: 26rpx;
  color: #999999;
  margin-right: 60rpx;
}
.user-unit-container .operate-cell .cell switch {
  transform: scale(0.8);
}
.user-unit-container .operate-cell .cell.link {
  position: relative;
}
.user-unit-container .operate-cell .cell.link:after {
  content: '\F00A';
  position: absolute;
  top: 50%;
  right: 32rpx;
  transform: translateY(-50%);
  font-family: 'vant-icon';
  color: #969799;
  font-size: 30rpx;
}
.user-unit-container .operate-cell .cell.link:active {
  background-color: #f2f3f5;
}
.user-unit-container .font21 {
  font-size: 21rpx!important;
}
.user-unit-container .van-nav-bar {
  background-image: none;
}
.user-unit-container .van-hairline--bottom:after {
  border: none;
}
.user-unit-container .van-cell__value {
  flex: none;
}
.user-unit-container .more {
  width: 50%;
  margin: 0 auto;
  height: 50rpx;
  line-height: 50rpx;
  background-color: #fff;
  border: 1px solid #f1f1f1;
  border-top: none;
  border-bottom-left-radius: 4px;
  border-bottom-right-radius: 4px;
  text-align: center;
  font-size: 32rpx;
  color: #666;
}
